/* phần header */
.header__navbar{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    overflow: hidden;
    height: 80px;
    background-color: #2f312e;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 20px;
    z-index: 999;
}
.header__navbar__img__link{
    text-decoration: none;
    user-select: none;
    -webkit-user-select: none;
    border: 1px solid var(--white-color);
    padding: 2px;
    border-radius: 3px;
    display: block;
}
.header__navbar__list{
    list-style: none;
    padding: 0;
    margin: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    user-select: none;
    -webkit-user-select: none;
}
.header__navbar__item__link:hover{
    color: var(--primary-color);
}
.header__navbar__item__link{
    font-size: 1.8rem;
    font-weight: 400;
    color: var(--white-color);
    display: block;
    text-decoration: none;
    padding: 16px;
    text-transform: uppercase;
    font-family: 'Permanent Marker', cursive;
    transition: 0.3s;
}
.header__navbar__talk-btn{
    border: 1px solid var(--primary-color);
    cursor: pointer;
    user-select: none;
    -webkit-user-select: none;
    transition: 0.3s;
    -webkit-transition: 0.3s;
    border-radius: 3px;
}
.header__navbar__talk-btn:hover{
    background-color:#3fa330;
    border-color: var(--white-color);
}
.header__navbar__talk-btn:hover .header__navbar__talk-btn__text,
.header__navbar__talk-btn:hover .header__navbar__talk-btn__icon{
    color: var(--white-color);
}
.header__navbar__talk-btn__icon{
    font-size: 2rem;
    padding: 12px;
    color: var(--primary-color);
    transition: 0.3s;
}
.header__navbar__talk-btn__text{
    font-size: 1.8rem;
    font-weight: 400;
    color: var(--primary-color);
    padding-right: 12px;
    text-transform: capitalize;
    transition: 0.3s;
}
.header__navbar__wrap-icon{
    display: flex;
    justify-content: center;
    align-items: center;
}
/* phần header__navbar__mb-tl */
.header__navbar__mb-tl{
    padding: 12px 0 12px 40px;
    position: relative;
    user-select: none;
    -webkit-user-select: none;
    -webkit-tap-highlight-color: transparent;
    display: none;
}
.header__navbar__mb-tl__icon{
    position: relative;
    width: 32px;
    height: 6px;
    background-color: var(--white-color);
    border-radius: 3px;
    margin: 4px 0;
    display: block;
    transition: 0.3s;
}
.header__navbar__mb-tl__icon::before{
    position: absolute;
    bottom: calc(100% + 6px);
    right: 0;
    content: "";
    width: 42px;
    height: 6px;
    background-color: var(--white-color);
    border-radius: 3px;
    display: block;
    transition: 0.3s;
}
.header__navbar__mb-tl__icon::after{
    position: absolute;
    top: calc(100% + 6px);
    right: 0;
    content: "";
    width: 42px;
    height: 6px;
    background-color: var(--white-color);
    border-radius: 3px;
    display: block;
    transition: 0.3s;
}
.header__navbar__mb-tl__list{
    position: fixed;
    top: 80px;
    left: 0;
    right: 0;
    bottom: 0;
    display: none;
    list-style: none;
    padding: 0;
    background-color: #181b17;
    padding: 100px 0;
    transition: 0.3s;
    animation: navbarAppear 0.3s linear;
    -webkit-animation: navbarAppear 0.3s linear;
}
@keyframes navbarAppear{
    0%{
        opacity: 0;
    }100%{
        opacity: 1;
    }
}
.header__navbar__mb-tl__item{
    margin: auto;
}
.header__navbar__mb-tl__link{
    font-size: 3rem;
    font-weight: 600;
    color: var(--primary-color);
    text-decoration: none;
    display: block;
    text-transform:uppercase;
    line-height: 3rem;
    padding: 30px 20px;
    text-align: center;
}
.header__navbar__mb-tl__link:hover{
    color: var(--white-color);
}
.click__navbar__mb-tl:checked ~ .header__navbar__mb-tl__list{
    display: block;
}
.click__navbar__mb-tl:checked ~ .header__navbar__mb-tl__icon{
    background-color: transparent;
}
.click__navbar__mb-tl:checked ~ .header__navbar__mb-tl__icon::before{
    display: block;
    transform: rotate(45deg);
    position: absolute;
    top: 0;
    background-color: red;
}
.click__navbar__mb-tl:checked ~ .header__navbar__mb-tl__icon::after{
    display: block;
    transform: rotate(-45deg);
    position: absolute;
    top: 0;
    background-color: red;
}


